---
import "../styles/resume.css";

interface Props {
  title?: string;
}

const { title = "Resume" } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>{title}</title>
  </head>

  <body>
    <main class="resume">
      <slot />
    </main>
  </body>
</html>
